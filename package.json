{
  "name": "muuuuminn-blog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "postbuild": "next-sitemap",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint:js": "eslint '**/*.@(js|ts|tsx)'",
    "check": "tsc && prettier --list-different . && npm run lint:js",
    "format": "prettier --write . && npm run lint:js -- --fix",
    "scaffdog": "scaffdog generate",
    "gen:deps": "depcruise --config .dependency-cruiser.js --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg",
    "gen:theme-typings": "npx @chakra-ui/cli tokens src/libs/chakra/theme/index.ts",
    "gen:type": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.11",
    "@chakra-ui/react": "2.3.7",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@tanstack/react-query": "^4.10.3",
    "axios": "^1.1.3",
    "date-fns": "2.29.3",
    "framer-motion": "7.6.4",
    "graphql": "^16.6.0",
    "gray-matter": "4.0.3",
    "next": "12.3.2",
    "next-seo": "5.14.0",
    "next-sitemap": "3.1.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtuoso": "^3.1.2",
    "recoil": "^0.7.6",
    "recoil-sync": "^0.1.1",
    "recoil-sync-next": "^0.0.2",
    "zenn-content-css": "0.1.131",
    "zenn-markdown-html": "0.1.131"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.10.0",
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/client-preset": "^1.0.5",
    "@graphql-codegen/near-operation-file-preset": "^2.4.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "dependency-cruiser": "11.18.0",
    "dotenv": "^16.0.3",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.2",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-check-updates": "16.3.16",
    "prettier": "2.7.1",
    "scaffdog": "^2.2.0",
    "typescript": "^4.8.4"
  }
