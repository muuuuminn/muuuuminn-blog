{
  "name": "muuuuminn-blog",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next -p 3333",
    "postbuild": "next-sitemap",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint:js": "eslint '**/*.@(js|ts|tsx)'",
    "check": "tsc && prettier --list-different . && npm run lint:js",
    "format": "prettier --write . && npm run lint:js -- --fix",
    "scaffdog": "scaffdog generate",
    "gen:deps": "depcruise --config .dependency-cruiser.js --include-only '^src' --output-type dot src | dot -T svg > dependencygraph.svg",
    "gen:theme-type": "npx @chakra-ui/cli tokens src/libs/chakra/theme/index.ts",
    "gen:graphql-type": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "eslint:rules": "eslint --print-config .eslintrc.js > eslint-rules.json",
    "gen:eslint-doc": "npm run eslint:rules && node script/show-eslint-config.js",
    "tsc:rules": "tsc --showConfig > tsconfig-result.json",
    "gen:tsconfig-doc": "npm run tsc:rules && node script/show-tsconfig-config.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@tanstack/react-query": "4.28.0",
    "@types/react-virtualized-auto-sizer": "1.0.1",
    "@types/react-window-infinite-loader": "1.0.6",
    "axios": "1.3.6",
    "date-fns": "2.29.3",
    "framer-motion": "10.11.2",
    "graphql": "16.6.0",
    "gray-matter": "4.0.3",
    "next": "12.3.4",
    "next-seo": "6.0.0",
    "next-sitemap": "4.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtualized-auto-sizer": "1.0.7",
    "react-virtuoso": "4.2.0",
    "react-window": "1.8.9",
    "react-window-infinite-loader": "1.0.9",
    "recoil": "0.7.7",
    "recoil-sync": "0.2.0",
    "recoil-sync-next": "0.0.4",
    "rss": "1.2.2",
    "zenn-content-css": "0.1.142",
    "zenn-markdown-html": "0.1.141"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.10.0",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@types/node": "18.15.13",
    "@types/react": "18.0.38",
    "@types/react-window": "1.8.5",
    "@types/rss": "0.0.30",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "dependency-cruiser": "12.11.3",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-config-next": "12.3.4",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "npm-check-updates": "16.10.9",
    "prettier": "2.8.7",
    "scaffdog": "2.5.0",
    "typescript": "4.9.5"
  }
}
