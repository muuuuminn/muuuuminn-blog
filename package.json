{
  "name": "next-emotion-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint:js": "eslint '**/*.@(js|ts|tsx)'",
    "check": "tsc && prettier --list-different . && npm run lint:js",
    "format": "prettier --write . && npm run lint:js -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ress": "5.0.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@types/node": "18.6.5",
    "@types/react": "18.0.17",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.21.0",
    "eslint-config-next": "^12.2.4",
    "eslint-config-prettier": "8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "16.0.5",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.@(ts|tsx)": "bash -c tsc",
    "*.@(js|ts|tsx)": "eslint --fix",
    "**/__tests__/?(*.)+(spec|test).[jt]s?(x)": "jest --ci"
  }
}
